                  <Font size="14.0" />
               </font>
            </Button>
            <ComboBox fx:id="champsGenre" layoutX="130.0" layoutY="133.0" prefHeight="25.0" prefWidth="173.0" promptText="Selectionnez un genre">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="Rock" />
                        <String fx:value="Blues" />
                        <String fx:value="Pop" />
                        <String fx:value="Funk" />
                        <String fx:value="Jazz" />
                        <String fx:value="Soul" />
                        <String fx:value="Electro" />
                        <String fx:value="Reggae" />


                    </FXCollections>
                </items>
            </ComboBox>
            <Text layoutX="88.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Genre" />
            <Text layoutX="88.0" layoutY="205.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Date" />
            <DatePicker fx:id="champsDate" layoutX="123.0" layoutY="196.0" promptText="Sélectionner une date" />
            <Text layoutX="93.0" layoutY="255.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Prix" />
            <TextField fx:id="champsMin" layoutX="130.0" layoutY="246.0" prefHeight="25.0" prefWidth="105.0" promptText="prix min" />
            <Text layoutX="249.0" layoutY="263.0" strokeType="OUTSIDE" strokeWidth="0.0" text="€" />
            <Text layoutX="270.0" layoutY="263.0" strokeType="OUTSIDE" strokeWidth="0.0" text="à" />
            <TextField fx:id="champsMax" layoutX="298.0" layoutY="247.0" prefHeight="25.0" prefWidth="105.0" promptText="prix max" />
            <MenuBar layoutY="1.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <CheckBox fx:id="checkboxDiscogs" layoutX="438.0" layoutY="90.0" mnemonicParsing="false" text="Discogs.com" />
            <CheckBox fx:id="checkboxFnac" layoutX="439.0" layoutY="125.0" mnemonicParsing="false" text="Fnac.com" />
            <CheckBox fx:id="checkboxVinylcorner" layoutX="440.0" layoutY="158.0" mnemonicParsing="false" text="Vinylecorner.fr" />
            <CheckBox fx:id="checkboxLeboncoin" layoutX="441.0" layoutY="192.0" mnemonicParsing="false" text="Leboncoin.fr" />
            <CheckBox fx:id="checkboxMesvinyles" layoutX="442.0" layoutY="221.0" mnemonicParsing="false" text="Mesvinyles.fr" />
            <CheckBox fx:id="checkboxCulturefactory" layoutX="443.0" layoutY="259.0" mnemonicParsing="false" text="Culturefactory.fr" />
         </children>
      </Pane>
   </children>

</VBox>
