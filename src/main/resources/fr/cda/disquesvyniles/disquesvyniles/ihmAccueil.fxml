<VBox alignment="CENTER" prefHeight="595.0" prefWidth="580.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.cda.disquesvyniles.disquesvyniles.VinyleController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Pane prefHeight="599.0" prefWidth="605.0">
         <children>
            <Text layoutX="36.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Recherche Vinyle" underline="true">
               <font>
                  <Font size="18.0" />
               </font>
            </Text>
            <Text layoutX="87.0" layoutY="103.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Titre" />
            <TextField fx:id="champsTitre" layoutX="121.0" layoutY="86.0" onAction="#effacerChamps" promptText="Selectionnez un artiste" />
            <Button fx:id="rechercherVinyle" layoutX="95.0" layoutY="308.0" mnemonicParsing="false" text="Rechercher">
               <font>
                  <Font size="14.0" />
